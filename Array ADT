#include <iostream>
using namespace std;
void display(struct Array);
  struct Array {
    int A[10];
    int size;
    int length;
  };
  
  int main() {
    Array arr;
    arr.A[0]=1;
    arr.A[1]=2;
      arr.A[2]=3;
      arr.A[3]=4;
      arr.A[4]=5;
    arr.length = 7;
    display(arr);
  }
   void display(struct Array arr) {
    int i, length;
    for (i = 0; i < arr.length; i++) {
      cout <<arr.A[i] << endl;
    }
   }
