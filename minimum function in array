#include <iostream>
using namespace std;
int MAx(int *A) {
  int min = A[0];
  for (int i = 1; i < 10; i++) {
    if (A[i] < min) {
      min = A[i];
    }
  }
  return min;
}
int main() {
  int A[] = {3, 6, 7, 8, 9, 5,1, 34, 65, 976,43};
  int length = 10;
  int r = MAx(A);
  cout << r << endl;
  return 0;
}
