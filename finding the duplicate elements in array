#include <iostream>
using namespace std;
int main(){
  int Array[]={1,2,3,3,4,5,6,6,7,8};
  int length=10;
  int duplicate=0;
  for(int i=0;i<length-1;i++){
    while(Array[i]==Array[i+1] && Array[i]!=duplicate){
      cout<<"Duplicate number is:"<<Array[i]<<endl;
      duplicate=Array[i];
    }
  }
  return 0;
}
