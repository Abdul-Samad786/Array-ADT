#include <iostream>
using namespace std;
int  set(int *A,int index,int x){
  if(index>=0 && index<10){
    A[index]=x;
  }
  for(int i=0;i<10;i++){
    cout<<A[i]<<endl;
  }
  return 0;
}
int main(){
  int A[]={3,6,7,8,5,34,65,76,876,76};
  int length=10;
  int r=set(A,4,27);
  cout<<r<<endl;
  return 0;
}
